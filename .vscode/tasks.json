{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Compilar troco.c",
      "type": "shell",
      "command": "gcc",
      "args": [
        "-std=c99",
        "-Wall",
        "-Wextra",
        "-g3",
        "${file}",
        "-o",
        "${workspaceFolder}/output/troco.exe"
      ],
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
